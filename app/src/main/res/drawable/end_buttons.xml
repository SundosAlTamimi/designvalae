<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#A373D6E6" />
            <stroke
                android:width="2dp"
                android:color="#A373D6E6" />
            <corners
                android:radius="5dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#f2e5e4"
                android:endColor="#e49e99"
                android:angle="90" />
            <!--<stroke-->
                <!--android:width="3dp"-->
                <!--android:color="#34abb5" />-->
            <corners
                android:radius="5dp" />

        </shape>
    </item>
</selector>