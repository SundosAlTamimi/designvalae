<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#a6e3e9" />
            <stroke
                android:width="15dp"
                android:color="#34abb5" />
            <corners
                android:radius="900dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>


    <item>
        <shape>
            <gradient
                android:startColor="#AE979595"
                android:endColor="#B2F5F5F5"
                android:angle="90" />
            <stroke
                android:width="3dp"
                android:color="#9E9E9E" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>