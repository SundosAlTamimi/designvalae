<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#B2FFFFFF"
                android:endColor="#B2FFFFFF"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="#FAFBFC" />
            <corners
                android:bottomLeftRadius="6dp"
                android:bottomRightRadius="6dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>